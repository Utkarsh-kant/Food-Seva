+package Fragments;
+
+import android.os.Bundle;
+
+import androidx.cardview.widget.CardView;
+import androidx.fragment.app.Fragment;
+
+import android.view.LayoutInflater;
+import android.view.View;
+import android.view.ViewGroup;
+
+import com.utkarsh.foodseva.R;
+
+
+public class DonateFragment extends Fragment {
+    CardView cardView_cooked,cardView_raw,cardView_packed;
+
+    View view;
+    @Override
+    public View onCreateView(LayoutInflater inflater, ViewGroup container,
+                             Bundle savedInstanceState) {
+        // Inflate the layout for this fragment
+      view= inflater.inflate(R.layout.fragment_donate, container, false);
+      cardView_cooked=view.findViewById(R.id.cardView_cooked);
+      cardView_packed=view.findViewById(R.id.cardView_packed);
+      cardView_raw=view.findViewById(R.id.cardView_raw);
+      cardView_cooked.setOnClickListener(view1 -> {
+          loadFragment(new CookedFoodFragment());
+      });
+      cardView_packed.setOnClickListener(view1 -> {
+          loadFragment(new PackedFoodFragment());
+      });
+      cardView_raw.setOnClickListener(view1 -> {
+          loadFragment(new RawFoodFragment());
+      });
+      return view;
+    }
+    private void loadFragment(Fragment fragment){
+        getActivity().getSupportFragmentManager().beginTransaction().replace(R.id.container,fragment).commit();
+    }
+
+
+
+   }
