+package Adapters;
+
+import static androidx.core.content.ContextCompat.startActivity;
+
+import android.content.Intent;
+import android.net.Uri;
+import android.view.LayoutInflater;
+import android.view.View;
+import android.view.ViewGroup;
+import android.widget.ImageView;
+import android.widget.TextView;
+
+import androidx.annotation.NonNull;
+import androidx.recyclerview.widget.RecyclerView;
+
+import com.utkarsh.foodseva.R;
+
+import java.util.ArrayList;
+
+import Models.ModelNgoList;
+
+public class ngoListRecycler extends RecyclerView.Adapter<ngoListRecycler.ViewHolder> {
+    ArrayList<ModelNgoList>NgoList;
+
+    public ngoListRecycler(ArrayList<ModelNgoList> ngoList) {
+        NgoList = ngoList;
+    }
+
+    @NonNull
+    @Override
+    public ngoListRecycler.ViewHolder onCreateViewHolder(@NonNull ViewGroup parent, int viewType) {
+        View view= LayoutInflater.from(parent.getContext()).inflate(R.layout.donation_item_list,parent,false);
+        ViewHolder viewHolder=new ViewHolder(view);
+        return viewHolder;
+    }
+
+    @Override
+    public void onBindViewHolder(@NonNull ngoListRecycler.ViewHolder holder, int position) {
+        holder.txt_ngo_name.setText(NgoList.get(position).name);
+        holder.txt_ngo_detail.setText(NgoList.get(position).detail);
+        holder.ngo_img.setImageResource(NgoList.get(position).img);
+        holder.itemView.setOnClickListener(new View.OnClickListener() {
+            @Override
+            public void onClick(View view) {
+                Intent intentUrl=new Intent(Intent.ACTION_VIEW);
+                intentUrl.setData(Uri.parse("https://ngodarpan.gov.in/index.php/home/statewise_ngo/70/4/1?per_page=100")) ;
+               startActivity(view.getContext(), intentUrl,null);
+            }
+        });
+
+    }
+
+    @Override
+    public int getItemCount() {
+        return NgoList.size();
+    }
+
+    public class ViewHolder extends RecyclerView.ViewHolder {
+        TextView txt_ngo_name,txt_ngo_detail;
+        ImageView ngo_img;
+        public ViewHolder(@NonNull View itemView) {
+            super(itemView);
+            txt_ngo_name=itemView.findViewById(R.id.txt_ngo_name);
+            txt_ngo_detail=itemView.findViewById(R.id.txt_ngo_detail);
+            ngo_img=itemView.findViewById(R.id.ngo_img);
+        }
+    }
+}
