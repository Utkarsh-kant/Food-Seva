+package Fragments;
+
+import android.annotation.SuppressLint;
+import android.content.Intent;
+import android.os.Bundle;
+
+import androidx.fragment.app.Fragment;
+
+import android.view.LayoutInflater;
+import android.view.View;
+import android.view.ViewGroup;
+
+import com.google.android.material.button.MaterialButton;
+import com.utkarsh.foodseva.R;
+
+
+public class HelpFragment extends Fragment {
+    private MaterialButton btn_report;
+
+  View view;
+    @SuppressLint("MissingInflatedId")
+    public View onCreateView(LayoutInflater inflater, ViewGroup container,
+                             Bundle savedInstanceState) {
+
+        View v= inflater.inflate(R.layout.help_activity, container, false);
+        btn_report=v.findViewById(R.id.btn_report);
+        btn_report.setOnClickListener(view->{
+            //Email intent passing
+            Intent igmail=new Intent(Intent.ACTION_SEND);
+            igmail.setType("text/plain");
+            Intent shareIntent = Intent.createChooser(igmail, null);
+            startActivity(shareIntent);
+        });
+        return v;
+    }
+}
