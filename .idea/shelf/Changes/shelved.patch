+package com.utkarsh.foodseva;
+
+import android.annotation.SuppressLint;
+import android.os.Bundle;
+import android.view.MenuItem;
+
+import androidx.activity.EdgeToEdge;
+import androidx.annotation.NonNull;
+import androidx.appcompat.app.AppCompatActivity;
+import androidx.fragment.app.Fragment;
+import androidx.fragment.app.FragmentManager;
+import androidx.fragment.app.FragmentTransaction;
+
+import com.google.android.material.bottomnavigation.BottomNavigationView;
+
+import Fragments.DonateFragment;
+import Fragments.HomeFragment;
+import Fragments.LocationFragment;
+import Fragments.OrganizationFragment;
+import Fragments.ProfileFragment;
+
+public class HomeActivity extends AppCompatActivity {
+    BottomNavigationView bottomNavigationView;
+
+
+
+
+    @SuppressLint("MissingInflatedId")
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        EdgeToEdge.enable(this);
+        setContentView(R.layout.activity_home);
+        bottomNavigationView=findViewById(R.id.bottomNavigationView);
+      
+
+        fragmentReplace(new HomeFragment());
+   bottomNavigationView.setOnNavigationItemSelectedListener(new BottomNavigationView.OnNavigationItemSelectedListener() {
+       @Override
+       public boolean onNavigationItemSelected(@NonNull MenuItem menuItem) {
+           int id=menuItem.getItemId();
+           if (id==R.id.nav_home){
+                fragmentReplace(new HomeFragment());
+                
+           }
+           else if (id==R.id.nav_donate){
+               fragmentReplace(new DonateFragment());
+
+           }
+           else if (id==R.id.nav_organization){
+               fragmentReplace(new OrganizationFragment());
+           }
+          else if (id==R.id.nav_profile){
+               fragmentReplace(new ProfileFragment());
+           }
+          else
+           fragmentReplace(new LocationFragment());
+           return true;
+
+       }
+   });
+
+      bottomNavigationView.setSelectedItemId(R.id.nav_home);
+
+
+
+    }
+   public void fragmentReplace(Fragment fragment){
+
+        FragmentManager fragmentManager=getSupportFragmentManager();
+        FragmentTransaction fragmentTransaction=fragmentManager.beginTransaction();
+        fragmentTransaction.replace(R.id.container,fragment);
+        fragmentTransaction.commit();
+
+    }
+
+
+
+      public void onBackPressed() {
+            Fragment currentFragment = getSupportFragmentManager().findFragmentById(R.id.container);
+            if (currentFragment instanceof HomeFragment)
+                super.onBackPressed();
+            else
+                bottomNavigationView.setSelectedItemId(R.id.nav_home);
+        }
+
+
+
+    }
