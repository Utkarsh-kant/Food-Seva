+import com.utkarsh.foodseva.HomeActivity;
+import com.utkarsh.foodseva.R;
+
+import java.util.Calendar;
+
+public class DetailOfDonateFragment extends Fragment {
+    private MaterialButton btn_save;
+    private EditText edt_user_location,edt_user_name,edt_phoneNo,edt_time;
+    private TextView txt_meal_type,txt_food_type;
+
+
+
+
+    public DetailOfDonateFragment() {
+        // Required empty public constructor
+    }
+
+
+    @Override
+    public View onCreateView(LayoutInflater inflater, ViewGroup container,
+                             Bundle savedInstanceState) {
+        // Inflate the layout for this fragment
+    View v= inflater.inflate(R.layout.fragment_detail_of_donate, container, false);
+        btn_save=v.findViewById(R.id.btn_save);
+        edt_time=v.findViewById(R.id.edt_time);
+        txt_meal_type = v.findViewById(R.id.txt_meal_type);
+        txt_food_type = v.findViewById(R.id.txt_food_type);
+
+          displaySelectedData();
+        //openingthe alert dialog box for saving the credentials
+        btn_save.setOnClickListener(view->{
+       AlertDialog.Builder builder=new AlertDialog.Builder(getContext());
+       builder.setMessage("The credential is saved Successfully");
+       builder.setTitle("Confirm");
+       builder.setCancelable(false);
+       builder.setIcon(R.drawable.ok);
+       builder.setPositiveButton("yes", new DialogInterface.OnClickListener() {
+           @Override
+           public void onClick(DialogInterface dialog, int which) {
+               HomeActivity homeActivity=(HomeActivity)getActivity();
+               homeActivity.fragmentReplace(new DonateFragment());
+               Toast.makeText(getContext(), "Your donation has been placed Sucessfully", Toast.LENGTH_SHORT).show();
+             dialog.cancel();
+           }
+       });
+       AlertDialog alertDialog= builder.create();
+alertDialog.show();
+
+        });
+
+        //opeing the calender to enter the date for donation
+        edt_time.setOnClickListener(view->{
+            Calendar calendar=Calendar.getInstance();
+            //geting year date and month
+
+                DatePickerDialog datePickerDialog=new DatePickerDialog(getContext(), new DatePickerDialog.OnDateSetListener() {
+                     @Override
+                     public void onDateSet(DatePicker view, int year, int month, int dayOfMonth) {
+                         edt_time.setText(dayOfMonth + "/" + (month + 1) + "/" + year);
+                     }
+                 },calendar.get(Calendar.YEAR),calendar.get(Calendar.MONTH),calendar.get(Calendar.DATE));
+                         datePickerDialog.show();
+            
+        });
+
+        
+    return v;
+    }
+    private void displaySelectedData() {
+        Bundle bundle = getArguments();
+
+        String value1 = bundle.getString("key1");
+        String value2 = bundle.getString("key2");
+
+            txt_food_type.setText(value1);
+            txt_meal_type.setText(value2);
+} }
