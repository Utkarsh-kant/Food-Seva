+import com.utkarsh.foodseva.R;
+
+import de.hdodenhof.circleimageview.CircleImageView;
+
+
+public class ProfileFragment extends Fragment {
+    static final int REQUEST_IMAGE_CAPTURE = 1;
+    FloatingActionButton btn_profile_img;
+    CircleImageView img_profile;
+    TextView txt_profile_donation_his,txt_profile_donation_sche,text_profile_help;
+
+    View view;
+
+    public View onCreateView(LayoutInflater inflater, ViewGroup container,
+                             Bundle savedInstanceState) {
+        // Inflate the layout for this fragment
+        view= inflater.inflate(R.layout.fragment_profile, container, false);
+        btn_profile_img=view.findViewById(R.id.btn_profile_img);
+        img_profile=view.findViewById(R.id.img_profile);
+        txt_profile_donation_his=view.findViewById(R.id.txt_profile_donation_his);
+        txt_profile_donation_sche=view.findViewById(R.id.txt_profile_donation_sche);
+        text_profile_help=view.findViewById(R.id.text_profile_help);
+        //Button click to open the activity
+       txt_profile_donation_his.setOnClickListener(new View.OnClickListener() {
+           @Override
+           public void onClick(View v) {
+               HomeActivity homeActivity=(HomeActivity) getActivity();
+               homeActivity.fragmentReplace(new ActivityFragment());
+
+
+           }
+       });
+        text_profile_help.setOnClickListener(view->{
+            HomeActivity homeActivity=(HomeActivity) getActivity();
+            homeActivity.fragmentReplace(new HelpFragment());
+        });
+
+
+        btn_profile_img.setOnClickListener(view1 -> {
+            open_galary();
+        });
+        return view;
+    }
+
+
+
+    //Code to open galary and to set the profile picture
+    public void open_galary(){
+        Intent intentGalary=new Intent(Intent.ACTION_PICK, MediaStore.Images.Media.EXTERNAL_CONTENT_URI);
+        startActivityForResult(intentGalary,REQUEST_IMAGE_CAPTURE);
+    }
+ public void onActivityResult(int requestCode, int resultCode, Intent data) {
+        super.onActivityResult(requestCode, resultCode, data);
+
+        if (requestCode == REQUEST_IMAGE_CAPTURE && resultCode == RESULT_OK && data != null) {
+            // Get the URI of the selected image
+            Uri selectedImageUri = data.getData();
+
+            // Set the selected image in the ImageView
+            img_profile.setImageURI(selectedImageUri);
+        }
+}  }
